{
  "name": "ai-browser",
  "version": "1.0.0",
  "description": "AI-powered lightweight browser with predictive hover and intelligent guidance",
  "main": "build/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite --port 3000",
    "dev:electron": "wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:preload",
    "build:renderer": "vite build",
    "build:preload": "vite build --config vite.preload.config.ts",
    "package": "npm run build && electron-builder",
    "package:win": "npm run build && electron-builder --win",
    "package:mac": "npm run build && electron-builder --mac",
    "package:linux": "npm run build && electron-builder --linux",
    "package:all": "npm run build && electron-builder --win --mac --linux",
    "dist": "npm run build && electron-builder --publish=never",
    "dist:win": "npm run build && electron-builder --win --publish=never",
    "dist:mac": "npm run build && electron-builder --mac --publish=never",
    "dist:linux": "npm run build && electron-builder --linux --publish=never",
    "publish": "npm run build && electron-builder --publish=always",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf build dist release",
    "clean:build": "rimraf build",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "prebuild": "npm run clean:build",
    "prepackage": "npm run clean:release"
  },
  "keywords": ["browser", "ai", "playwright", "electron", "automation"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.0",
    "express": "^4.18.0",
    "playwright": "^1.40.0",
    "openai": "^4.20.0"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.1.0",
    "typescript": "^5.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "wait-on": "^7.0.0",
    "cross-env": "^7.0.0",
    "rimraf": "^5.0.0",
    "eslint": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "jest": "^29.0.0"
  },
  "build": {
    "extends": "./electron-builder.config.js"
  }
}